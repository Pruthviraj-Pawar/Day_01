day_number = int(input("Enter the digit from 1 to 7 to know the day"))

if(day_number==1):
    print("Monday")
elif(day_number==2):
    print("Tuesday")
elif(day_number==3):
    print("Wednesday")
elif(day_number==4):
    print("Thursday")
elif(day_number==5):
    print("Friday")
elif(day_number==6):
    print("saturday")
elif(day_number==7):
    print("Sunday")
else:
    print("Invalid day number")
